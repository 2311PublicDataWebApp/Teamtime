<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.teamtime.tt.team.model.mapper.TeamMapper">
	<insert id="insertTeam">
		INSERT INTO TEAM_TBL VALUES(SEQ_TEAM_NO.NEXTVAL, #{userId }, 1, #{teamName })
		<selectKey keyProperty="teamNo" resultType="int" order="AFTER">
			SELECT SEQ_TEAM_NO.CURRVAL FROM DUAL
		</selectKey>
	</insert>
	<insert id="insertUserTeam">
		INSERT INTO USER_TEAM_TBL VALUES(#{teamNo }, #{userId })
	</insert>
</mapper>